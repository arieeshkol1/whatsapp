        "meta_endpoint": "https://graph.facebook.com/",
        "bedrock_agent_id": null,
        "bedrock_agent_alias_id": null,
        "bedrock_agent_foundation_model_id": "amazon.nova-lite-v1:0",
        "bedrock_agent_inference_profile_arn": null
      }
    }
